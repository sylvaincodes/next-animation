{
  "name": "next-animation",
  "description": "C'est une librarie Javascript pour cr√©er des animations scroll incroyables",
  "version": "1.0.2",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "github",
    "react",
    "repositories",
    "animation",
    "scroll",
    "stars"
  ],
  "author": "Adande sylvain <sylvaincodeur@gmail.com>",
  "homepage": "https://github.com/sylvaincodes/next-animation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylvaincodes/next-animation.git"
  },

  "files": [
		"dist"
	],
  "main": "./dist/next-animation.cjs.js",
  "typings": "./dist/index.d.ts",
	"module": "./dist/next-animation.es.js",
	"exports": {
		".": {
			"import": "./dist/next-animation.es.js",
			"require": "./dist/next-animation.cjs.js",
			"types": "./dist/index.d.ts"
		},
    "./scroll": {
			"import": "./dist/scroll/next-animation.es.js",
			"require": "./dist/scroll/next-animation.cjs.js",
			"types": "./dist/scroll/index.d.ts"
		},
		"./style.css": "./dist/style.css"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*"
			]
		}
	},
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "next-animation": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"

  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  }
}
